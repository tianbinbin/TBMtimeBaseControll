source 'https://github.com/CocoaPods/Specs.git'

# 此处是管理cocoapod第三方库的显示状态 加上这两句之后 看起来更有层次了
install! 'cocoapods', :generate_multiple_pod_projects => true

platform :ios, '11.0'
use_frameworks!

target 'TBMtimeBaseControll' do

  pod 'Alamofire', '5.2.1'	
  pod 'R.swift', '5.2.2'
  pod 'RxSwift', '5.1.1'
  pod 'RxCocoa', '5.1.1'
  pod 'RxDataSources', '4.0.1'
  pod 'RxGesture', '3.0.2'
  pod 'SnapKit', '5.0.1'


end
